{
    "extends": "@repo/typescript-config/base.json",
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true, 
      "strict": true,
      "skipLibCheck": true,
      "declaration": false,
      "declarationMap": false,

    },
    "exclude": ["node_modules"],
    "include": [".", "src", "types", "src/routes"]
}